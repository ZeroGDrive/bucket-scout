{
  "$schema": "../gen/schemas/desktop-schema.json",
  "identifier": "default",
  "description": "enables the default permissions",
  "windows": ["main"],
  "permissions": [
    "core:default",
    "store:default",
    "fs:default",
    "fs:allow-write-file",
    "fs:allow-remove",
    "fs:allow-stat",
    "fs:scope-temp-recursive",
    "fs:allow-temp-write-recursive"
  ]
}
